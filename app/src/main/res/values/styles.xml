<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomNavTextStyle" parent="TextAppearance.AppCompat.Caption">
        <item name="fontFamily">@font/noto_sans_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="BottomNavNoIndicator" parent="">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
    </style>
    <style name="CustomDialog" parent="Theme.MaterialComponents.Dialog.Alert">
        <item name="android:background">#2E2E2E</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>


</resources>
